{
  "scope": "juicyfx",
  "name": "packagist",
  "rewrites": [
    { "source": "/favicon.ico", "destination": "https://juicy.vercel.app/favicon.ico" },
    { "source": "/stats/([\\w\\-]+)/([\\w\\-]+)(/?)", "destination": "/api/stats/package.ts?vendor=$1&package=$2" },
    { "source": "/stats/([\\w\\-\\+]+)(/?)", "destination": "/api/stats/organization.ts?vendor=$1" },
    { "source": "/", "destination": "https://juicy.vercel.app/readme/packagist" }
  ]
}

{
    "version": 2,
    "scope": "juicyfx",
    "name": "podfuk",
    "alias": ["podfuk.jfx.cz"],
    "builds": [
        { "src": "api/*.ts", "use": "@now/node" },
        { "src": "package.json", "use": "@now/static-build", "config": { "distDir": "dist" } }
    ],
    "routes": [
        { "src": "^/url/(.*)", "dest": "/api/url.ts" },
        { "src": "^/raw/(.*)", "dest": "/api/raw.ts" },
        { "src": "^/post/(.*)", "dest": "/api/post.ts" }
    ]
}
